    pod 'ObjectMapper'#json转model
    pod 'Moya'#对Alamofire更高层次网络请求的封装抽象层
	pod 'Moya-ObjectMapper'
    pod 'Moya-ObjectMapper/RxSwift'

  # Pods for RxSwift_MVVM_demo

  target 'RxSwift_MVVM_demoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RxSwift_MVVM_demoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
